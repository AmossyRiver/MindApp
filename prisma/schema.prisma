generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model EncryptedEntry {
  id            String   @id @default(cuid())
  userId        String
  encryptedData Bytes
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt

  @@index([userId, createdAt])
}
